<!DOCTYPE html>
<html lang="cs">
<head>
    <meta charset="utf-8">
    <!--Import Google Icon Font-->
    <link href="{$basePath}/css/icon.css" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="{$basePath}/css/main.css?1" media="screen,projection"/>

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta charset="utf-8">
    {*<title>{ifset title}{include title|stripHtml} | {/ifset}ALBIXON</title>*}
    <title>Foto Albixon</title>

    <link rel="apple-touch-icon" sizes="76x76" href="{$basePath}/images/bata_ikona_3_76.jpg"/>
    <link rel="apple-touch-icon" sizes="120x120" href="{$basePath}/images/bata_ikona_3_120.jpg"/>
    <link rel="apple-touch-icon" sizes="152x152" href="{$basePath}/images/bata_ikona_3_152.jpg"/>
    <link rel="apple-touch-icon" sizes="180x180" href="{$basePath}/images/bata_ikona_3_180.jpg"/>

    <link rel="icon" sizes="16x16" href="{$basePath}/images/bata_ikona_3_16.jpg"/>
    <link rel="icon" sizes="32x32" href="{$basePath}/images/bata_ikona_3_32.jpg"/>
    <link rel="icon" sizes="96x96" href="{$basePath}/images/bata_ikona_3_96.jpg"/>
    <link rel="icon" sizes="192x192" href="{$basePath}/images/bata_ikona_3_192.jpg"/>
    <link rel="shortcut icon" href="{$basePath}/images/bata_ikona_3_16.jpg"/>


    <meta name="msapplication-TileColor" content="#fffff"/>
    <meta name="msapplication-TileImage" content="{$basePath}/images/bata_ikona_96.jpeg"/>
    <meta name="msapplication-config" content="{$basePath}/images/browserconfig.xml"/>
</head>
<body>
<nav>
    <div class="nav-wrapper">
        <ul id="nav-mobile">
            <li n:class="$presenter->isLinkCurrent('Homepage:default') ? active, isset($type) ? checked" id="co-fotit">
                {if isset($type)}
                    <a href="{link Homepage:default, type => null, op => null}" class="done item">Co chce코 fotit&nbsp;<i
                                class="material-icons tiny">done</i><span n:if="isset($type)"
                                                                          class="second-line">{$type["name"]}</span></a>
                {else}
                    <a href="{link Homepage:default}">Co chce코 fotit</a>
                {/if}
            </li>
            <li n:class="$presenter->isLinkCurrent('Homepage:op') ? active, isset($op) && isset($type) ? checked">
                {if isset($type) && isset($op)}
                    <a href="{link Homepage:op, op => null}" class="done item">Vyber OP&nbsp;<i
                                class="material-icons tiny">done</i>
                        <span class="second-line">{$op}</span></a>
                {else}
                    <span>Vyber OP</span>
                {/if}
            </li>
            <li n:class="$presenter->isLinkCurrent('Homepage:photoform') ? active">
                {if isset($type) && isset($op)}
                    <a href="{link Homepage:photoform}">Fo콘</a>
                {else}
                    <span>Fo콘</span>
                {/if}
            </li>
        </ul>
    </div>
</nav>

<div n:foreach="$flashes as $flash" n:class="flash, $flash->type">{$flash->message}</div>
<div class="container">
    {include content}
</div>
{block scripts}
    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="{$basePath}/js/jquery-2.1.1.min.js"></script>
    {*<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>*}
    <script type="text/javascript" src="{$basePath}/js/materialize.min.js"></script>
    <script src="{$basePath}/js//netteForms.min.js"></script>
    <script type="text/javascript" src="{$basePath}/js/jquery.ui.widget.js"></script>
    <script src="{$basePath}/js/load-image.all.min.js"></script>
    <script src="{$basePath}/js/canvas-to-blob.min.js"></script>
    <script type="text/javascript" src="{$basePath}/js/jquery.fileupload.js"></script>
    <script type="text/javascript" src="{$basePath}/js/jquery.iframe-transport.js"></script>
    <script type="text/javascript" src="{$basePath}/js/jquery.fileupload-process.js"></script>
    <script type="text/javascript" src="{$basePath}/js/jquery.fileupload-image.js"></script>
    <script type="text/javascript" src="{$basePath}/js/jquery.fileupload-validate.js"></script>
    {*<script type="text/javascript" src="{$basePath}/js/jquery.fileupload-ui.js"></script>*}
    <script type="text/javascript" src="{$basePath}/js/main.js?1"></script>
    {*{if \Tracy\Debugger::$productionMode && preg_match('/podnety.albixon.cz/', strtolower($_SERVER['HTTP_HOST']))}*}
    {*<script src="{$basePath}/js/ga.js?{time()|noescape}"></script>*}
    {*{/if}*}
{/block}
</body>
</html>
